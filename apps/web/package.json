{
  "name": "@summa/web",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "npm run test:cov",
    "test:proj:node-core": "NODE_OPTIONS=\"--max-old-space-size=8192 --expose-gc\" vitest --workspace ./vitest.workspace.ts run --project node-core --pool=forks --reporter=basic",
    "test:proj:scrolly": "NODE_OPTIONS=\"--max-old-space-size=8192 --expose-gc\" vitest --workspace ./vitest.workspace.ts run --project scrolly --pool=forks --reporter=basic",
    "test:proj:viz:contract": "NODE_OPTIONS=\"--max-old-space-size=8192 --expose-gc\" vitest --workspace ./vitest.workspace.ts run --project viz --pool=forks --reporter=basic lib/viz/adapters/adapters.contract.test.tsx",
    "test:proj:viz:events": "NODE_OPTIONS=\"--max-old-space-size=8192 --expose-gc\" vitest --workspace ./vitest.workspace.ts run --project viz --pool=forks --reporter=basic lib/viz/events.test.ts",
    "test:proj:viz:fake": "NODE_OPTIONS=\"--max-old-space-size=8192 --expose-gc\" vitest --workspace ./vitest.workspace.ts run --project viz --pool=forks --reporter=basic tests/viz.fake-chart.spec.tsx",
    "test:proj:viz": "npm run -s test:proj:viz:contract && npm run -s test:proj:viz:events && npm run -s test:proj:viz:fake",
    "test:proj:ui": "NODE_OPTIONS=\"--max-old-space-size=8192 --expose-gc\" vitest --workspace ./vitest.workspace.ts run --project ui --pool=forks --reporter=basic",
    "test:quick": "npm run -s test:proj:node-core && npm run -s test:proj:scrolly && npm run -s test:proj:viz && npm run -s test:proj:ui",
    "test:cov": "npm run -s test:proj:node-core -- --coverage && npm run -s test:proj:scrolly -- --coverage && npm run -s test:proj:viz && npm run -s test:proj:ui -- --coverage",
    "viz:check": "eslint --ext .ts,.tsx lib/viz && tsc --project tsconfig.viz.json --noEmit",
    "viz:size": "node ./scripts/viz-size.mjs"
  },
  "dependencies": {
    "@sentry/nextjs": "^7.120.4",
    "@tanstack/react-table": "^8.21.3",
    "@visx/scale": "^3.12.0",
    "@visx/shape": "^3.12.0",
    "@visx/xychart": "^3.12.0",
    "arquero": "^8.0.3",
    "d3-array": "^3.2.4",
    "d3-scale": "^4.0.2",
    "deck.gl": "^9.2.2",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "focus-visible": "^5.2.1",
    "gray-matter": "^4.0.3",
    "idb-keyval": "^6.2.2",
    "intersection-observer": "^0.12.2",
    "klaro": "^0.7.21",
    "luma.gl": "^7.3.2",
    "maplibre-gl": "^5.10.0",
    "next": "^14.2.8",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.3.0",
    "perfume.js": "^9.4.0",
    "plausible-tracker": "^0.3.9",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-vega": "^8.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "swr": "^2.3.6",
    "vega": "^6.2.0",
    "vega-embed": "^7.1.0",
    "vega-lite": "^6.4.1",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.33",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-istanbul": "^2.1.1",
    "@vitest/coverage-v8": "^2.1.1",
    "autoprefixer": "^10.4.17",
    "babel-loader": "8.4.1",
    "clean-webpack-plugin": "4.0.0",
    "jest-axe": "^9.0.0",
    "jsdom": "^25.0.1",
    "next-pwa": "5.6.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.5.4",
    "vitest": "^2.1.1",
    "workbox-webpack-plugin": "6.6.0",
    "workbox-window": "6.6.0"
  },
  "version": "0.1.1",
  "overrides": {},
  "resolutions": {},
  "msw": {
    "workerDirectory": [
      "apps/web/public"
    ]
  }
}
